                            document.addEventListener("keydown", e => {
                                const tagName = document.activeElement.tagName.toLowerCase()
                              
                                if (tagName === "input") return
                              
                                switch (e.key.toLowerCase()) {
                                  case " ":
                                    if (tagName === "button") return
                                  case "k":
                                    togglePlay()
                                    break
                                  case "f":
                                    toggleFullscreen()
                                    break
                                  case "t":
                                    toggleTheaterMode()
                                    break
                                  case "i":
                                    toggleMiniPlayerMode()
                                    break
                                  case "m":
                                    toggleMute()
                                    break
                                  case "arrowleft":
                                  case "j":
                                    skip(-5)
                                    break
                                  case "arrowright":
                                  case "l":
                                    skip(5)
                                    break
                                  case "c":
                                    toggleCaptions()
                                    break
                                }
                              })